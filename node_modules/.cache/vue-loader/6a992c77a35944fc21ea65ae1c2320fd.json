dicators background=\"#ababab\"\n                    img-width=\"1024\" img-height=\"480\" style=\"text-shadow: 1px 1px 2px #333;\"\n                    @sliding-start=\"onSlideStart\" @sliding-end=\"onSlideEnd\">\n                    <!-- Text slides with image -->\n                    <b-carousel-slide caption=\"First slide\"\n                        text=\"Nulla vitae elit libero, a pharetra augue mollis interdum.\"\n                        img-src=\"https://picsum.photos/1024/480/?image=52\"></b-carousel-slide>\n\n                    <!-- Slides with custom text -->\n                    <b-carousel-slide img-src=\"https://picsum.photos/1024/480/?image=54\">\n                        <h1>Hello world!</h1>\n                    </b-carousel-slide>\n                </b-carousel>\n            </div>\n        </div>\n\n\n        <div>\n            <a href=\"#intro\"><img src=\"../assets/godown.png\" alt=\"\" class=\"godown\" /></a>\n        </div>\n        <picture>\n            <!-- <source media=\"(max-width: 450px)\" :srcset=\"banner_img.mobile\" />\n            <source media=\"(max-width: 768px)\" :srcset=\"banner_img.tablet_ver\" />\n            <source media=\"(max-width: 1024px)\" :srcset=\"banner_img.tablet_hor\" /> -->\n            <!-- <img :src=\"banner_img.desktop\" /> -->\n        </picture>\n    </div>\n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            banner_img: {\n                // desktop: require(`../assets/testttt.jpg`),\n                // tablet_hor: require(`../assets/banner1024_500.jpg`),\n                // tablet_ver: require(`../assets/banner768_500.jpg`),\n                // mobile: require(`../assets/banner450_500.jpg`),\n            },\n            title: `民視新聞網`,\n            logo: require('../assets/logo_ftv.svg'),\n            slide: 0,\n            sliding: null\n\n        }\n    },\n\n}\n</script>\n\n<!-- 閉包 -->\n<style scoped>\np {\n    font-size: 18px;\n}\n\n.banner {\n    height: 100vh;\n    width: 100%;\n    background-size: cover;\n    background-image: url(../assets/banner1920x1080.jpg);\n}\n\nh1 {\n\n    font-size: 60px;\n    text-align: center;\n}\n\nh2 {\n    font-size: 36px;\n}\n\n.logo {\n    margin-top: 20px;\n    width: 40px;\n    margin-left: 3rem;\n}\n\nh3 {\n    margin-top: -3px;\n    color: black;\n    font-size: 28px;\n}\n\nh3 a {\n    margin-top: -5px;\n    color: rgb(6, 59, 138);\n    font-size: 28px;\n}\n\n.title {\n    margin: auto;\n    margin-top: 15vh;\n    font-size: 50px;\n    line-height: 75px;\n\n}\n\n.subtitle {\n    margin-right: 300px;\n    margin: auto;\n    margin-top: 13vh;\n    line-height: 40px;\n    font-size: 24px;\n    text-align: center;\n}\n\n@media screen and (max-width: 768px) {\n    .title {\n        margin-top: 30vh;\n    }\n\n    .subtitle {\n        margin-top: 20vh;\n    }\n}\n\np {\n    font-size: 16px;\n    /* color: var(--main_color); */\n    line-height: 24px;\n    color: black;\n    margin: auto;\n}\n\n\nimg {\n    height: 30px;\n    margin-right: 5px;\n}\n\n.logo img {\n    display: flex;\n    font-size: 30px;\n}\n\nspan {\n    display: flex;\n    font-size: 30px;\n}\n\n@keyframes bounce2 {\n\n    0%,\n    20%,\n    50%,\n    80%,\n    100% {\n        transform: translate(-50%, 0);\n    }\n\n    40% {\n        transform: translate(-50%, -30px);\n    }\n\n    60% {\n        transform: translate(-50%, -15px);\n    }\n}\n\n.godown:hover {\n    animation: bounce2 1.5s ease infinite;\n}\n\n.godown {\n    height: 3.5rem;\n    position: absolute;\n    bottom: 5em;\n    transform: translateX(-50%);\n}\n\n.hashtag {\n    display: flex;\n    margin: auto;\n    justify-content: center;\n    cursor: pointer;\n}\n\n.hashtag {\n    display: flex;\n    margin: auto;\n    justify-content: center;\n    cursor: pointer;\n}\n</style>\n"]}]}