 infoModal2.showModal()\n        })\n        let close2 = document.querySelector('#close2')\n        close2.addEventListener('click', function () {\n            infoModal2.close()\n        })\n        let btn3 = document.querySelector('#show3')\n        let infoModal3 = document.querySelector('#infoModal3')\n        btn3.addEventListener('click', function () {\n            infoModal3.showModal()\n        })\n        let close3 = document.querySelector('#close3')\n        close3.addEventListener('click', function () {\n            infoModal3.close()\n        })\n    },\n}\n</script>\n\n<style scoped>\n.layout {\n    background-color: var(--layout_background);\n}\n\n.layout_grid {\n    width: 800px;\n    height: 800px;\n    background-color: rgb(255, 255, 255);\n    margin-top: 3rem;\n    margin-bottom: 4rem;\n    border-radius: 10px;\n}\n\n.layout_grid_news img {\n    width: 100%;\n    height: 500px;\n    border-radius: 10px;\n}\n\n.layout_grid_news .grid_info h4 {\n    background-color: white;\n    overflow: hidden;\n    white-space: nowrap;\n    text-overflow: ellipsis;\n    display: -webkit-box;\n    -webkit-line-clamp: 2;\n    font-size: 28px;\n    -webkit-box-orient: vertical;\n    white-space: normal;\n    max-height: 80px;\n    line-height: 2.3rem;\n    padding: 10px 1rem 25px;\n}\n\n.layout_grid_news .grid_info p {\n    margin: 0;\n    color: #161615;\n    background-color: white;\n    text-align: start;\n    padding: 10px 1rem 25px;\n}\n\n.layout_grid_news .grid_cover img {\n    width: 100%;\n\n    background-color: white;\n}\n\n.layout_grid_news .grid_cover {\n    max-height: 400px;\n    overflow: hidden;\n    background-color: white;\n}\n\na {\n    color: black;\n    font-size: px;\n    line-height: 150%;\n}\n\np {\n    text-align: right;\n    padding-right: 1rem;\n}\n\nbutton {\n    margin-top: -10px;\n\n    font-size: 20px;\n    background-color: rgb(248, 216, 221);\n    border: transparent 1px solid;\n}\n\n.show {\n    display: none;\n}\n\n.title_icon {